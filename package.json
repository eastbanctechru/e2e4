    "chai": "3.5.0",
    "coveralls": "^2.11.11",
    "cz-conventional-changelog": "^1.1.6",
    "del": "^2.2.1",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-gh-pages": "^0.5.4",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tslint": "^6.0.1",
    "gulp-typedoc": "^2.0.0",
    "gulp-typescript": "^2.13.6",
    "istanbul-instrumenter-loader": "^0.2.0",
    "karma": "^1.1.1",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "1.1.1",
    "karma-phantomjs-launcher": "1.0.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "1.7.0",
    "merge2": "^1.0.2",
    "mocha": "3.0.0",
    "object.assign": "^4.0.4",
    "phantomjs-prebuilt": "2.1.9",
    "pre-commit": "^1.1.3",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.2.2",
    "sinon": "2.0.0-pre",
    "ts-loader": "0.8.2",
    "tslint": "^3.13.0",
    "typedoc": "^0.4.4",
    "typescript": "^1.8.10",
    "typings": "^1.3.2",
    "vinyl": "^1.1.1",
    "vinyl-paths": "^2.1.0",
    "wallaby-webpack": "0.0.22",
    "webpack": "1.13.1",
    "yargs": "^4.8.1"
  },
