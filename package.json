{
  "name": "e2e4",
  "version": "1.0.0-beta.16",
  "description": "Set of base classes and utilities to build unobtrusive list models",
  "jsnext:main": "esm/index.js",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fshchudlo/e2e4.git"
  },
  "keywords": [],
  "author": "Fedor Shchudlo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fshchudlo/e2e4/issues"
  },
  "homepage": "https://github.com/fshchudlo/e2e4#readme",
  "scripts": {
    "precommit-check": "gulp prepublish",
    "precommit-msg": "echo 'Run pre-commit checks...' && exit 0",
    "ci": "gulp test",
    "postinstall": "typings install",
    "coveralls": "cat ./reports/coverage/lcov.info | ./node_modules/.bin/coveralls"
